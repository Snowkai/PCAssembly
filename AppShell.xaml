<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PCAssembly.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PCAssembly"
    Shell.FlyoutBehavior="Disabled"
    Title="PCAssembly">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Config"
        ContentTemplate="{DataTemplate local:Pages.PCConfig}"
        Route="PCConfig" />

</Shell>
